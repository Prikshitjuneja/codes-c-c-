void preOrder(BinaryTreeNode<int> *root) {
	// Write your code here
	if(root==NULL)
	return;

	cout<<root->data<<' ';
	preOrder(root->left);
	preOrder(root->right);
}

void postOrder(BinaryTreeNode<int> *root) {
	// Write your code here
	if(root==NULL)
	return;
	postOrder(root->left);
	
	postOrder(root->right);
	
	cout<<root->data<<' ';
        
}
