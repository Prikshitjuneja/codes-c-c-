#include<iostream>
using namespace std;
int main() {
	int x;
    cin>>x;
    
    int i=1;
    int count=0;
    while(i<=x+count)
    {
        int d=3*i+2;
        if(d%4!=0)
        {
            cout<<d<<' ';
            
        }
        else
        {
            count++;
        }
        i++;
    }
}
