#include<stdio.h>
void search(int arr[],int n,int key);
int main()
{int n=5;
    int arr[5]={5,4,87,65,78};
    int key;
    printf("enter the value u want to search\n");
    scanf("%d",&key);
    search(arr,n,key);
}
void search(int arr[],int n,int key)
{
     int k=-1;
    for(int i=0;i<n;i++)
    {

        if(key==arr[i])
        {
           k=i;
            break;
        }

    }
   printf("index at which value is present is %d\n",k);
}
